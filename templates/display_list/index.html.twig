{% extends 'base.html.twig' %}

{% block title %}Test d'affichage{% endblock %}

{% block body %}




    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nom de l'établissement</th>
                <th scope="col">Type</th>
                <th scope="col">Public/Privé</th>
                <th scope="col">Adresse</th>
                <th scope="col">Code postal</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td scope="row">{{ data.records.0.record.fields.nom_etablissement }}</th>
                <td>{{ data.records.0.record.fields.type_etablissement }}</td>
                <td>{{ data.records.0.record.fields.statut_public_prive }}</td>
                <td>{{ data.records.0.record.fields.adresse_1 }}</td>
                <td>{{ data.records.0.record.fields.code_postal }}</td>
